{
  "name": "openarena-bsp-parser",
  "version": "0.1.0",
  "description": "This library can be used to parse a OpenArena BSP files",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && copy-pkg package.json dist/package.json --ignore devDependencies,scripts",
    "build:w": "tsc -w",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "ts-mocha src/**/*.spec.ts"
  },
  "author": "Daniel Leib",
  "repository": {
    "type": "git",
    "url": "git@github.com:mucer/openarena-bsp-parser.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.10",
    "copy-pkg": "^1.0.0",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-mocha": "^2.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {}
}
